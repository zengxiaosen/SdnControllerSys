[buildfile]
  includes = //buck-tools/default.defs

[plugins]
  directory = //bin/plugins

[java]
  source_level = 8
  target_level = 8

[alias]
    onos = //tools/package:onos-package
    onos-local = //tools/package:onos-run

[download]
  in_build = true

[maven_repositories]
  central = https://maven.aliyun.com/nexus/content/groups/public/

[project]
  ide = intellij
  ignore = .git
